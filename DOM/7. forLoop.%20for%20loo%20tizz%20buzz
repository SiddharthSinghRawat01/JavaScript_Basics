var output = [];
// var count = 1;//count first value
function fizzBuzz(){
for(var count = 1;count<=100; count++){
    
 if (count % 3===0 && count % 5===0){
        output.push("tizzBuzz");
    }
    else if(count % 3===0){
        output.push("tizz")
    } 
    else if(count % 5===0){
        output.push("Buzz")
    }
    else {
        output.push(count);
    }
    }
// count++
    console.log(output)
}
